fn invert_the_case(s: String) -> String {
    s.chars()
        .map(|c| {
            if c.is_lowercase() {
                c.to_uppercase().to_string()

            } else if c.is_uppercase() {
                c.to_lowercase().to_string()
            } else {
                c.to_string()

            }

        })

        .collect()

}
fn main() {
    let input = "Привіт, Hello!";
    let output = invert_the_case(input.to_string());
    println!("{}", output); 
